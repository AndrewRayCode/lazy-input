{
  "name": "lazy-input",
  "version": "1.0.9",
  "description": "A lazy React.js input field that only updates when it is told to re-render (fixes issues with Flux backed field data)",
  "main": "dist/LazyInput.js",
  "scripts": {
    "build": "./scripts/build",
    "test": "./scripts/test",
    "test": "./node_modules/.bin/mocha --require ./babel.js --compilers jsx:babel/register --recursive ./src/__tests__",
    "test-cov": "./scripts/test-cov",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HurricaneJames/lazy-input"
  },
  "keywords": [
    "react",
    "react-component",
    "flux",
    "input",
    "textarea",
    "lazy",
    "optimistic",
    "render"
  ],
  "author": "James Burnett <HurricaneJamesEsq@gmail.com> (https://github.com/HurricaneJames/lazy-input)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HurricaneJames/lazy-input/issues"
  },
  "homepage": "https://github.com/HurricaneJames/lazy-input",
  "devDependencies": {
    "babel": "^4.6.6",
    "expect.js": "^0.3.1",
    "istanbul": "~0.3.7",
    "jsdom": "^3.1.0",
    "mocha": "^2.0.1",
    "react": "^0.13.2",
    "sinon": "^1.14.1"
  }
}
